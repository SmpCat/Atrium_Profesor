<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- FICHERO DE CONFIGURACION DE SPRING -->
	<!-- SOLO CON VALOR DOCUMENTATIVO -->
	<!-- ****** AUTOR: JUAN ANTONIO SOLVES GARCIA ****** -->
	<!-- ****** FECHA: 21-11-2016 ****** -->
	<!-- ****** VERSION: 2.0 ****** -->

	<!-- INYECCION EN PROPIEDADES COMPUESTAS -->

	<!-- SET -->
	<bean id="identificador objeto" class="tipo/clase objeto">
		<property name="nombre_propiedad">
			<set>
				<value></value>
				<ref></ref>
			</set>
		</property>
	</bean>
	<!-- LIST -->
	<bean id="identificador objeto" class="tipo/clase objeto">
		<property name="nombre_propiedad">
			<list></list>
		</property>
	</bean>
	<!-- PROPERTIES -->
	<bean id="identificador objeto" class="tipo/clase objeto">
		<property name="nombre_propiedad">
			<props>
				<prop key=""></prop>
			</props>
		</property>
	</bean>
	<!-- MAPAS -->
	<bean id="identificador objeto" class="tipo/clase objeto">
		<property name="nombre_propiedad">
			<map>
				<entry key="" value-ref="" />
			</map>
		</property>
	</bean>
	<!-- TABLAS -->
	<bean id="identificador objeto" class="tipo/clase objeto">
		<property name="nombre_propiedad">
			<array>
				<value></value>
			</array>
		</property>
	</bean>

</beans>
